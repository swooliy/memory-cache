        "swoole",
        "cache",
        "memory"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ney Chang",
            "email": "zoobileg@gmail.com",
            "homepage": "https://github.com/zairl23"
        }
